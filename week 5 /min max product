#include <iostream>
using namespace std ;
int main() {
    int n=5;
    int arr1[n]={0,0,5,0,0};
    int arr2[n]={12,5,4,56,20};
    int largest=0;
    for(int i=0;i<n;i++){
    if(arr1[i]>largest)
    {
        largest = arr1[i];
    }
    }
    
    int smallest = arr2[0];
    for(int i=0;i<n;i++){
    if(arr2[i]<smallest)
    {
        smallest = arr2[i];
    }
}
cout << largest*smallest;
    return 0;
}
