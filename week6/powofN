// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;

int powofnum(int num,int power){
    if(power == 0){
        return 1;
    }
    return num * powofnum(num,power-1);
}

int main() {
    int num ;
    cin >> num ;
    int power ;
    cin >> power ;
    cout << powofnum(num,power);

    return 0;
}
