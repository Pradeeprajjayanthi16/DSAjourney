
#include <iostream>
using namespace std ;

int main() {
    int a[] = {54,23,1,34,43,89,76,20};
    int n = 8 ;
    for(int j=0 ; j < n-1 ; j++){
        for(int i=0 ; i < n-1 ; i++){
            if(a[i]>a[i+1]){
                swap(a[i],a[i+1]);
            }
        }
    }
for(int i=0 ; i < n ; i++){
    cout << a[i] << endl ;

}
    return 0;
}
