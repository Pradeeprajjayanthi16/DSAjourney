#include <iostream>
using namespace std ;
int main() {
    int n = 5,even=0,odd=0;
    int arr[10] ;
    for(int j=0;j<n;j++){
        cin >> arr[j];
    }
    for(int i=0;i<n;i++){
    if(arr[i]%2==0)
    {
        even++;
    }
    else{
        odd++;
    }
    }
    cout << even << endl << odd ;

    return 0;
}
